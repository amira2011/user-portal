<h1>Your Leads</h1>
<div data-controller="lead">
<button type="button" class="btn btn-primary">Primary</button>
<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Phone</th>
     </tr>
  </thead>
  <tbody>
    <% @leads.each do |lead| %>
      <tr>
        <td><%= lead.first_name %></td>
        <td><%= lead.last_name %></td>
        <td><%= lead.email %></td>
        <td><%= lead.phone %></td>
       </tr>
    <% end %>
  </tbody>
</table>
 <h1> This is home page </h1>
